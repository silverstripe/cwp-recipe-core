{
    "name": "cwp/cwp-recipe-core",
    "type": "silverstripe-recipe",
    "description": "CWP core requirements recipe",
    "homepage": "https://www.cwp.govt.nz",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-core": "4.8-dev",
        "cwp/cwp-core": "2.8-dev",
        "silverstripe/auditor": "2.x-dev",
        "silverstripe/environmentcheck": "2.x-dev",
        "silverstripe/hybridsessions": "2.x-dev"
    },
    "require-dev": {
        "sminnee/phpunit": "^5.7"
    },
    "extra": {
        "project-files": [
            "app/*"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
